{
    "env": {
        "sketchBookPath": "${env:USERPROFILE}/Documents/Arduino",
        "arduinoDataPath": "${env:LOCALAPPDATA}/Arduino15"
    },
    "configurations": [
        {
            "name": "ARM",
            "intelliSenseMode": "gcc-x64",
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "ARDUINO_SAMD_ARCH",
                "SAMD21G18A"
            ],
            "includePath": [
                "${workspaceFolder}/Nemesis-Mod/**",
                "${sketchBookPath}/libraries/DualG2HighPowerMotorShield",
                "${arduinoDataPath}/packages/adafruit/hardware/samd/1.2.9/cores/arduino",
                "${arduinoDataPath}/packages/adafruit/hardware/samd/1.2.9/variants/feather_m0",
                "${arduinoDataPath}/packages/arduino/tools/CMSIS-Atmel/1.2.0/CMSIS/Device/ATMEL"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "${workspaceFolder}/.vscode/temp/cache",
                "path": [
                    "${sketchBookPath}/libraries/DualG2HighPowerMotorShield/*",
                    "${arduinoDataPath}/packages/adafruit/hardware/samd/1.2.9/cores/arduino/*",
                    "${arduinoDataPath}/packages/adafruit/hardware/samd/1.2.9/variants/feather_m0/*",
                    "${arduinoDataPath}/packages/arduino/tools/CMSIS-Atmel/1.2.0/CMSIS/Device/ATMEL/*"
                ]
            },
            "compilerPath": "${arduinoDataPath}/packages/arduino/tools/arm-none-eabi-gcc/4.8.3-2014q1/arm-none-eabi/bin/gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}