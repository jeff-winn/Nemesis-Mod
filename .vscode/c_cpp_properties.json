{
    "env": {
        "sketchBookPath": "${env:USERPROFILE}/Documents/Arduino",
        "arduinoDataPath": "${env:LOCALAPPDATA}/Arduino15/packages"
    },
    "configurations": [
        {
            "name": "ARM",
            "intelliSenseMode": "gcc-x64",
            "defines": [
                "F_CPU=48000000L",
                "ARDUINO=10808",
                "ARDUINO_ARCH_SAMD",
                "ARDUINO_SAMD_ZERO",
                "ARM_MATH_CM0PLUS",
                "ADAFRUIT_FEATHER_M0",
                "__arm__",
                "__SAMD21G18A__"
            ],
            "includePath": [
                "${workspaceFolder}/**",
                "${sketchBookPath}/libraries/DualG2HighPowerMotorShield",
                "${sketchBookPath}/libraries/Adafruit_PWM_Servo_Driver_Library",
                "${sketchBookPath}/libraries/Arduino_Low_Power/src",
                "${sketchBookPath}/libraries/RTCZero/src",
                "${arduinoDataPath}/adafruit/hardware/samd/1.2.9/cores/arduino",
                "${arduinoDataPath}/adafruit/hardware/samd/1.2.9/variants/feather_m0",
                "${arduinoDataPath}/arduino/hardware/samd/1.6.20/libraries/Wire",
                "${arduinoDataPath}/arduino/tools/CMSIS-Atmel/1.2.0/CMSIS/Device/ATMEL",
                "${arduinoDataPath}/arduino/tools/CMSIS/4.5.0/CMSIS/Include"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/temp/cache",
                "path": [
                    "${sketchBookPath}/libraries/DualG2HighPowerMotorShield/*",
                    "${sketchBookPath}/libraries/Adafruit_PWM_Servo_Driver_Library/*",
                    "${sketchBookPath}/libraries/Arduino_Low_Power/src/*",
                    "${sketchBookPath}/libraries/RTCZero/src/*",
                    "${arduinoDataPath}/adafruit/hardware/samd/1.2.9/cores/arduino/*",
                    "${arduinoDataPath}/adafruit/hardware/samd/1.2.9/variants/feather_m0/*",
                    "${arduinoDataPath}/arduino/hardware/samd/1.6.20/libraries/Wire/*",
                    "${arduinoDataPath}/arduino/tools/CMSIS-Atmel/1.2.0/CMSIS/Device/ATMEL/*",
                    "${arduinoDataPath}/arduino/tools/CMSIS/4.5.0/CMSIS/Include/*"
                ]
            },
            "compilerPath": "${arduinoDataPath}/arduino/tools/arm-none-eabi-gcc/4.8.3-2014q1/bin/arm-none-eabi-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}